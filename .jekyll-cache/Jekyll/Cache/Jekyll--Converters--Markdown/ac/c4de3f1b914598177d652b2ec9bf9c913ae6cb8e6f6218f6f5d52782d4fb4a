  <span class="n">scores</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="mi">80</span><span class="o">);</span>

  <span class="c1">//ì •ë ¬ëœ Integer ê°ì²´ë¥¼ í•˜ë‚˜ì”© ê°€ì ¸ì˜¤ê¸°</span>
  <span class="k">for</span><span class="o">(</span><span class="nc">Integer</span> <span class="n">s</span> <span class="o">:</span> <span class="n">scores</span><span class="o">){</span>
   <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">s</span><span class="o">+</span><span class="s">" "</span><span class="o">);</span>
  <span class="o">}</span>
