  <span class="o">}</span>
  <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">();</span>

  <span class="c1">//í‚¤ë¡œ ì—”íŠ¸ë¦¬ ì‚­ì œ</span>
  <span class="n">map</span><span class="o">.</span><span class="na">remove</span><span class="o">(</span><span class="s">"í™ê¸¸ë™"</span><span class="o">);</span>
