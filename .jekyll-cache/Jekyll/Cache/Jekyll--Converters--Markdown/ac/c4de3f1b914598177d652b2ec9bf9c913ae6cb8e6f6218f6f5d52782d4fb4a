  <span class="n">map</span><span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">"í™ê¸¸ë™"</span><span class="o">,</span> <span class="mi">95</span><span class="o">);</span> <span class="c1">// í‚¤ê°€ ê°™ê¸° ë•Œë¬¸ì— ì œì¼ ë§ˆì§€ë§‰ì— ì €ì¥í•œ ê°’ë§Œ ì €ì¥</span>
  <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì´ Entry ìˆ˜ : "</span><span class="o">+</span><span class="n">map</span><span class="o">.</span><span class="na">size</span><span class="o">());</span>
  <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">();</span>

